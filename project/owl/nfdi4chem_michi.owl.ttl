@prefix OBI: <http://purl.obolibrary.org/obo/OBI_> .
@prefix PATO: <http://purl.obolibrary.org/obo/PATO_> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix linkml: <https://w3id.org/linkml/> .
@prefix michi: <https://w3id.org/NFDI4Chem/MIChI> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema1: <http://schema.org/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

michi:Analysis a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "Analysis" ;
    skos:definition "The planned process to analyse/assay a sample." ;
    skos:exactMatch OBI:0001209 ;
    skos:inScheme <https://w3id.org/StroemPhi/NFDI4Chem_MIChI> .

michi:ChemicalSubstance a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "Chemical_Substance" ;
    skos:inScheme <https://w3id.org/StroemPhi/NFDI4Chem_MIChI> .

michi:DatasetCollection a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "DatasetCollection" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom michi:Dataset ;
            owl:onProperty michi:entries ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty michi:entries ] ;
    skos:definition "A holder for Dataset objects" ;
    skos:inScheme <https://w3id.org/StroemPhi/NFDI4Chem_MIChI> .

michi:Molecule a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "Molecule" ;
    skos:definition "A molecular entity" ;
    skos:inScheme <https://w3id.org/StroemPhi/NFDI4Chem_MIChI> .

michi:Sample a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "Sample" ;
    skos:definition "The material entity that is being evaluated." ;
    skos:inScheme <https://w3id.org/StroemPhi/NFDI4Chem_MIChI> .

michi:Dataset a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "Dataset" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty michi:primary_email ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty michi:primary_email ],
        [ a owl:Restriction ;
            owl:allValuesFrom [ a rdfs:Datatype ;
                    owl:onDatatype xsd:string ;
                    owl:withRestrictions ( [ xsd:pattern "^\\S+@[\\S+\\.]+\\S+" ] ) ] ;
            owl:onProperty michi:primary_email ],
        michi:NamedThing ;
    skos:definition "Represents a Dataset" ;
    skos:inScheme <https://w3id.org/StroemPhi/NFDI4Chem_MIChI> .

michi:NamedThing a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "NamedThing" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty michi:description ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty michi:name ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty michi:id ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty michi:description ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty michi:name ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:Uriorcurie ;
            owl:onProperty michi:id ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty michi:id ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty michi:description ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty michi:name ] ;
    skos:definition "A generic grouping for any identifiable entity" ;
    skos:exactMatch schema1:Thing ;
    skos:inScheme <https://w3id.org/StroemPhi/NFDI4Chem_MIChI> .

PATO:0001421 a owl:Class,
        michi:PersonStatus ;
    rdfs:label "ALIVE" ;
    rdfs:subClassOf michi:PersonStatus .

PATO:0001422 a owl:Class,
        michi:PersonStatus ;
    rdfs:label "DEAD" ;
    rdfs:subClassOf michi:PersonStatus .

<https://w3id.org/NFDI4Chem/MIChIPersonStatus#UNKNOWN> a owl:Class,
        michi:PersonStatus ;
    rdfs:label "UNKNOWN" ;
    rdfs:subClassOf michi:PersonStatus .

michi:entries a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "entries" ;
    skos:inScheme <https://w3id.org/StroemPhi/NFDI4Chem_MIChI> .

michi:description a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "description" ;
    skos:definition "A human-readable description for a thing" ;
    skos:inScheme <https://w3id.org/StroemPhi/NFDI4Chem_MIChI> .

michi:id a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "id" ;
    rdfs:range linkml:Uriorcurie ;
    skos:definition "A unique identifier for a thing" ;
    skos:inScheme <https://w3id.org/StroemPhi/NFDI4Chem_MIChI> .

michi:name a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "name" ;
    skos:definition "A human-readable name for a thing" ;
    skos:inScheme <https://w3id.org/StroemPhi/NFDI4Chem_MIChI> .

michi:PersonStatus a owl:Class,
        linkml:EnumDefinition ;
    owl:unionOf ( PATO:0001421 PATO:0001422 <https://w3id.org/NFDI4Chem/MIChIPersonStatus#UNKNOWN> ) ;
    linkml:permissible_values PATO:0001421,
        PATO:0001422,
        <https://w3id.org/NFDI4Chem/MIChIPersonStatus#UNKNOWN> .

<https://w3id.org/StroemPhi/NFDI4Chem_MIChI> a owl:Ontology ;
    rdfs:label "MIChI" ;
    dcterms:license "MIT" ;
    dcterms:title "MIChI" ;
    rdfs:seeAlso michi: ;
    skos:definition "This is the metadata schema for NFDI4Chem" .

